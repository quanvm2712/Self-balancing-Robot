#ifndef	_MAX7221_H
#define _MAX7221_H

/**
 * MAX7221 register definition
 */
#define		DIGIT_O		0X1
#define		DIGIT_1		0X2
#define		DIGIT_2		0X3
#define		DIGIT_3		0X4
#define		DIGIT_4		0X5
#define		DIGIT_5		0X6
#define		DIGIT_6		0X7
#define		DIGIT_7		0X8
#define 	DecodeMode	0x9
#define		Intensity	0xA
#define		ScanLimit	0xB
#define		Shutdown	0xC
#define 	DisplayTest	0xD





#endif //__MAX7219_H
